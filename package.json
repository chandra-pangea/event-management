{
  "name": "event-management",
  "version": "1.0.0",
  "description": "Backend System for a Virtual Event Management Platform",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "NODE_ENV=test jest --forceExit",
    "test:coverage": "NODE_ENV=test jest --coverage --forceExit"
  },
  "keywords": [
    "event",
    "management",
    "virtual",
    "platform"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.1",
    "nodemailer": "^6.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "jest-mock": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  }
}